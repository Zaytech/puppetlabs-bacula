Pool {
  Name = "<%= name -%>"
  Pool Type = "<%= type -%>"
  Recycle = "<%= recycle -%>"          
  AutoPrune = "<%= autoprune -%>"            
  Volume Retention = <%= volumeretention -%>    	#Volume retencao
  Maximum Volume Bytes = <%= maximumvolumebytes -%>  	#Capacidade maxima do volume
  Maximum Volumes = <%= maximumvolumes -%>		#Quantidade maxima de volume
  LabelFormat = <%= labelformat -%>			#Label dos volumes 
}
